<r-cell class="border-left u-desktop-only" span="1-2" span-s="row"></r-cell>
<r-cell
    class="border-left-right work--header-empty u-desktop-only"
    span="3-12"
    span-s="row"
    span-md="row"
>
</r-cell>

<r-cell
    class="border-left-bottom u-desktop-only"
    span="1-2"
    span-s="row"
    span-md="row"
></r-cell>
<r-cell
    class="work--header-container border-left-right"
    span="3-12"
    span-s="row"
    span-md="row"
>
    <header class="header border-bottom" role="banner">
        <div class="header--inner">
            <div class="header--branding">
                <p class="body-large">
                    <a class="logo" href="{{ '/' | url }}">
                        <img
                            src="{{
                                '/assets/images/afnizar-nur-ghifari.png' | url
                            }}"
                            alt="Afnizar Nur Ghifari"
                            width="36"
                            height="36"
                            loading="lazy"
                    /></a>
                </p>
            </div>
            <div class="header--nav">{% include "nav.njk" %}</div>
        </div>
    </header>
</r-cell>

